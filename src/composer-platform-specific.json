{
    "name": "aotd/mystem-binaries",
    "version": "3.0",
    "license": "MIT",
    "extra": {
        "platform-specific-packages": {
            "aotd/mystem-binaries-3": [
                {"yandex/mystem-mac": "dev-master", "os": "macosx"},
                {"yandex/mystem-linux64": "dev-master", "os": "linux", "architecture": "x64"},
                {"yandex/mystem-windows64": "dev-master", "os": "windows", "architecture": "x64"}
            ]
        }
    },
    "repositories": [
        {
            "type":"package",
            "package":{
                "name":"yandex/mystem-mac",
                "version":"dev-master",
                "bin": ["mystem"],
                "dist":{ "url":"https://download.cdn.yandex.net/mystem/mystem-3.1-macosx.tar.gz", "type":"tar" }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"yandex/mystem-linux64",
                "version":"dev-master",
                "bin": ["mystem"],
                "dist":{"url":"https://download.cdn.yandex.net/mystem/mystem-3.1-linux-64bit.tar.gz", "type":"tar" }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"yandex/mystem-windows64",
                "version":"dev-master",
                "bin": ["mystem.exe"],
                "dist":{"url":"https://download.cdn.yandex.net/mystem/mystem-3.1-win-64bit.zip", "type":"zip" }
            }
        }
    ]
}
